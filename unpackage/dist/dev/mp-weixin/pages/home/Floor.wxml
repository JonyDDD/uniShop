<view class="floor-list"><block wx:for="{{floorList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="floor-item"><image class="floor-title" src="{{item.floor_title.image_src}}"></image><view class="floor-img-box"><navigator class="left-img-box" url="{{item.product_list[0].toUrl}}"><image style="{{'width:'+(item.product_list[0].image_width+'rpx')+';'}}" src="{{item.product_list[0].image_src}}" mode="widthFix" class="_img"></image></navigator><view class="right-img-box"><block wx:for="{{item.product_list}}" wx:for-item="item2" wx:for-index="i2" wx:key="i2"><block wx:if="{{i2!==0}}"><navigator class="right-img-item" url="{{item2.toUrl}}"><image style="{{'width:'+(item2.image_width+'rpx')+';'}}" src="{{item2.image_src}}" mode="widthFix"></image></navigator></block></block></view></view></view></block></view>